class TheGirl:
    lastname = "Allina"
    def __init__(self, name):
        self.fullname = name + self.lastname
    
    def fun(selfself, where):
        print("%s는 술 먹으러 %s에 갔네" % (self.fullname, where))
        
    def love(self, other):
        print("%s, %s 사랑에 빠졌다<3" % (self.fullname, other.fullname))
        
        